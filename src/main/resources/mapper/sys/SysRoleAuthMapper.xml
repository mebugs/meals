<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meals.sys.mapper.SysRoleAuthMapper">
    <select id="getRoleAuthIds"  resultType="java.lang.Long">
        SELECT a.aid FROM `sys_role_auth` a WHERE a.rid = ${id}
    </select>
</mapper>
